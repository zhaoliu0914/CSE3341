procedure t9 is
	procedure A(a, b) is
		array n;
		a[0] := a[0]+b[0];
		b := array n;
	end
	procedure S(a, b) is
		array n;
		a[0] := a[0]-b[0];
		begin A(a, b);
		b := array n;
	end
	array x;
	array y;
begin
	x := new integer[1];
	x[0] := 1;
	y := new integer[1];
	y := 1+x;
	begin S(x, y);
	out(x);
end
